    <script type="text/javascript">
        $(document).ready(function () {
            var i = 1;
            debugger;
            $.ajax({
                url: '/Menu/Menu/',
                method: 'get',
                dataType: 'json',
                async: false,
                success: function (data) {
                    debugger;
                    buildMenu($('#side-menu'), data);
                    debugger;
                }
            });

            function buildMenu(parent, items) {
                debugger;
                $.each(items, function () {
                    debugger;
                    if (this.MenuList.length == 0) {
                        var li = $('<li class="Get"><a class="my-link" href='+this.Link+'><i class="glyphicon ' + this.LabelIcon + '"></i> ' + this.Label + '</a><span style="display:none;">' + "-" + this.Id + "," + '</span></li>');
                    } else {
                        var li = $('<li class="Get"><a class="my-link" href="#"><i class="glyphicon ' + this.LabelIcon + '"></i> ' + this.Label + '<span class="glyphicon arrow"></span></a><span style="display:none;">' + "," + '</span></li>');
                    }
                    debugger;
                    li.appendTo(parent);
                    debugger;
                    if (this.ParentId == 0) {
                        i = 1;
                    }
                    if (this.MenuList && this.MenuList.length > 0) {
                        var ul = $('<ul></ul>');
                        if (i == 1) {
                            ul.addClass("nav nav-second-level");
                            i++;
                        } else if (i == 2) {
                            ul.addClass("nav nav-third-level");
                            i++;
                        } else if (i == 3) {
                            ul.addClass("nav nav-fourth-level");
                            i++;
                        }
                        ul.appendTo(li);
                        buildMenu(ul, this.MenuList);
                    }
                });
            }
        });

    </script>
